<div id="<%= dom_id article %>" class="article">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= article.url %>
  </p>
  <p>
  <strong>Created at:</strong>
    <%= article.created_at.strftime("%A, %Y-%m-%d %H:%M")%>
  </p>

  <p>
  <strong>Votes_Up:</strong>
    <%= article.votes_up%>
  </p>

  <p>
  <strong>Votes_Down:</strong>
    <%= article.votes_down%>
  </p>

</div>
